	"packageManager": "pnpm@10.14.0",
	"devDependencies": {
		"@biomejs/biome": "^2.2.3",
		"@types/node": "^24.3.1",
		"@vitest/coverage-v8": "^3.2.4",
		"modivo": "link:.",
		"typescript": "^5.9.2",
		"unplugin-dts": "1.0.0-beta.6",
		"vite": "^7.1.4",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.4"
	}

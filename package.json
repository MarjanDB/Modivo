{
	"name": "modivo",
	"version": "0.0.1",
	"description": "Dependency Injection Library",
	"author": {
		"name": "Mario Moƒçnik"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/MarjanDB/Modivo.git"
	},
	"license": "AGPL-3.0-or-later",
	"type": "module",
	"files": [
